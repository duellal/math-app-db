    "dependencies": {
        "axios": "^1.13.2",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "knex": "^3.1.0",
        "knex-cleaner": "^1.3.1",
        "pg": "^8.16.3",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "husky": "^9.1.7",
